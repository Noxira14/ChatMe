  }

  let dt = text.split("|");
  if (dt.length < 3) {
    return bot.sendMessage(
      chatId,
      "‚ö†Ô∏è Format salah.\n\nContoh:\n`/editsrv 1|apikey|APIKEY_BARU`",
      { parse_mode: "Markdown" }
    );
  }

  let [idx, field, newValue] = dt;
  let index = parseInt(idx) - 1;

  if (isNaN(index) || index < 0 || index >= serverpanel.length) {
    return bot.sendMessage(chatId, "‚ö†Ô∏è Nomor server ga valid.");
  }

  // validasi field
  let validFields = ["domain", "egg", "nestid", "loc", "apikey", "capikey"];
  if (!validFields.includes(field)) {
    return bot.sendMessage(
      chatId,
      `‚ö†Ô∏è Field *${field}* ga valid.\n\nYang bisa diedit: ${validFields.join(", ")}`,
      { parse_mode: "Markdown" }
    );
  }

  // kalau domain ‚Üí pastiin tambah https://
  if (field === "domain") {
    newValue = `https://${newValue}`;
    if (serverpanel.some((s, i) => s.domain === newValue && i !== index)) {
      return bot.sendMessage(chatId, "‚ö†Ô∏è Domain itu udah dipake server lain bos.");
    }
  }

  // update data
  serverpanel[index][field] = newValue;
  fs.writeFileSync(filePath, JSON.stringify(serverpanel, null, 2));

  bot.sendMessage(
    chatId,
    `‚úÖ Data server nomor *${idx}* berhasil diupdate.\nField *${field}* ‚Üí \`${newValue}\``,
    { parse_mode: "Markdown" }
  );
